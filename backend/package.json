{
  "name": "backend",
  "version": "1.0.0",
  "description": "Express backend for PDF extraction with Groq LLM",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env PORT=3001 node src/index.js",
    "dev": "cross-env PORT=3001 nodemon src/index.js",
    "build": "echo 'Build step for backend' && npm install",
    "test": "jest",
    "accuracy:download": "node test/accuracy/downloadSamplePlans.js",
    "accuracy:run": "node test/accuracy/watershedAccuracyTest.js --verbose",
    "accuracy:report": "node test/accuracy/generateReport.js",
    "test:api": "jest --testPathPattern=api",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "groq-sdk": "^0.3.0",
    "helmet": "^7.1.0",
    "json2csv": "^5.0.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "cross-env": "^10.0.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}
